{
  "name": "pika-pack-template",
  "version": "0.0.2",
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg",
        {
          "exclude": [
            "__tests__/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-deno"
      ]
    ]
  },
  "pre-commit": "check-commit",
  "scripts": {
    "test": "jest",
    "lint": "eslint src/**/*s",
    "check-commit": "lint-staged && tsc && jest --changedSince=HEAD",
    "build": "pika build"
  },
  "dependencies": {
    "@pika/fetch": "^0.1.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-deno": "^0.9.2",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-build-web": "^0.9.2",
    "@pika/plugin-ts-standard-pkg": "^0.9.2",
    "@pika/types": "^0.9.2",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "commitlint": "^11.0.0",
    "eslint": "^7.16.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "pre-commit": "^1.2.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ],
    "*.js": [
      "eslint --fix"
    ],
    "*.css": [
      "stylelint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
